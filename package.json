{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Node server. Supports Mocha for testing, JSX syntax, Linting etc.",
  "main": "src/server.jsx",
  "scripts": {
    "watch:build": "webpack -wd",
    "watch:server": "nodemon --exec babel-node src/index.jsx",
    "watch:server:debug": "nodemon --inspect-brk --exec babel-node src/index.jsx --ignore public/",
    "test": "mocha --require babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saleen34/node-boilerplate.git"
  },
  "keywords": [
    "Node",
    "Mocha",
    "JSX",
    "Boilerplate",
    "Linting"
  ],
  "author": "Nick Pate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saleen34/node-boilerplate/issues"
  },
  "homepage": "https://github.com/saleen34/node-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint-config-airbnb-standard": "^1.4.1",
    "nodemon": "^1.12.1",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0",
    "should": "^13.1.0"
  }
}
